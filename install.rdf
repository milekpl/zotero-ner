<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>zotero-ner-author-normalizer@marcinmilkowski.pl</em:id>
    <em:name>Zotero NER Author Name Normalizer</em:name>
    <em:version>1.0.0</em:version>
    <em:description>Normalize author names using NER to handle different formats</em:description>
    <em:creator>Marcin Milkowski</em:creator>
    <em:homepageURL>https://github.com/marcinmilkowski/zotero-ner</em:homepageURL>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>true</em:unpack>
    
    <!-- Zotero 7 -->
    <em:targetApplication>
      <Description>
        <em:id>zotero@zotero.org</em:id>
        <em:minVersion>7.0</em:minVersion>
        <em:maxVersion>7.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>