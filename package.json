    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.40.0",
    "babel-jest": "^29.0.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.36.0",
    "jest": "^29.0.0",
    "playwright": "^1.40.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "onnxruntime-web": "^1.14.0"
  }
