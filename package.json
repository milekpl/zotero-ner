{
  "name": "zotero-ner-author-normalizer",
  "version": "1.0.0",
  "description": "Zotero extension for NER-based author name normalization",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.json",
    "lint:fix": "eslint . --ext .js,.json --fix",
    "test": "npm run test:unit && npm run ui-test",
